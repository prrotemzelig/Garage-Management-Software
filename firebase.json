{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "cors": [
    {
      "origin": [
        "http://garage-management-softwa.appspot.com"
      ],
      "method": [
        "GET"
      ],
      "responseHeader": [
        "Content-Type"
      ],
      "maxAgeSeconds": 3600
    }
  ]
}
